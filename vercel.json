{
    "version": 2,
    "name": "disqusjs-proxy",
    "alias": [
        "disqus.eallion.com"
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "cache-control": "public, max-age=21600",
                "Access-Control-Allow-Origin":"https://eallion.com"
            },
            "dest": "https://disqus.com/api/$1"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true
    }
}
